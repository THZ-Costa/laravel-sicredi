{
    "name": "thz-costa/laravel-sicredi",
    "description": "A package to create charges from SICREDI API",
    "keywords": ["sicredi", "laravel", "bank"],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ThzCosta\\LaravelSicredi\\": "src/"
        }
    },
    
    "authors": [
        {
            "name": "Thomaz Costa"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^7.8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "sicredi\\SicrediServiceProvider"
            ],
            "aliases": {
                "Iza": "sicredi\\Facades\\Sicredi"
            }
        }
    }
}
